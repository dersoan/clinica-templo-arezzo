# Configuração para Hostinger
# Copie este arquivo para .env e configure com seus dados reais

# Banco PostgreSQL (pegar no cPanel da Hostinger)
DATABASE_URL=postgresql://seu_usuario:sua_senha@localhost:5432/clinica_arezzo

# Session Secret (gerar uma chave aleatória segura)
SESSION_SECRET=sua_chave_muito_longa_e_aleatoria_aqui_123456789

# Ambiente de produção
NODE_ENV=production

# Configurações do PostgreSQL separadas (caso necessário)
PGHOST=localhost
PGPORT=5432
PGDATABASE=clinica_arezzo
PGUSER=seu_usuario_postgres
PGPASSWORD=sua_senha_postgres